loc_8483:				
		CLI
		LDA	#-1
		STA	byte_15
		BNE	loc_848F

loc_848A:				
		LDA	#2        ;"Unknown error"
		JSR	get_error	; A = error code

loc_848F:
                jsr maybe_verbose_exit_on_error
                JMP	language_entry.command_loop

.pend
                
