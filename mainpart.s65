		LDA	(byte_C),Y	; input	char
		CMP	#$41 ; 'A'
		BCC	not_alpha
		CMP	#$5B ; '['
		BCS	not_alpha
		LDA	(byte_E),Y	; command table	char
		BEQ	didnt_match	; end of command?
