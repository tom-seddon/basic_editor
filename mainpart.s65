KEEP_DETOKENIZE_ONE_LINE_MEMSET = false
KEEP_9E1D_MEMSET = false

; I strip out the memmove calls one at a time...
KEEP_B8DB_MEMMOVE = false
KEEP_BF73_MEMMOVE = false
KEEP_A8C0_MEMMOVE = false

