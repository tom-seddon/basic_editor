                             ;letters (0x61...)
		BCS	isdigit.done
		CMP	#'Z'+1
		BCS	clc_then_rts
		CMP	#'A'
		BCS	isdigit.done
                ; fall through
                .pend



; sets C if A is a digit char

isdigit:.proc			
