		LDA	current_line_no_lsb
		PHA
		LDA	current_line_no_msb
		PHA
		TXA
		PHA
		JSR	sub_AA47
		LDA	#0
		STA	byte_3B
		LDA	current_line_no_lsb
		STA	byte_2B
		LDA	current_line_no_msb
